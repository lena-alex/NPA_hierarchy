Cell[CellGroupData[{

Cell[TextData[StyleBox["The code ",
 FontSize->24]], "Section",
 CellChangeTimes->{{3.663478852467031*^9, 3.663478854783263*^9}, 
   3.663479456715706*^9},
 FontFamily->"Century Schoolbook",
 FontSize->12,
 FontWeight->"Plain",ExpressionUUID->"f7ec601e-259f-487f-8c9b-49d41bf7b61a"],

Cell[BoxData[
 StyleBox[
  RowBox[{
   RowBox[{
    RowBox[{"NPAmat", "[", 
     RowBox[{"level_", ",", "parties_", ",", "inputs_", ",", "outputs_"}], 
     "]"}], ":=", "\[IndentingNewLine]", 
    RowBox[{"Module", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", " ", 
       RowBox[{
       "allrules", ",", "Generators", ",", "LTuples", ",", "NumberLTuples", 
        ",", "joinTuples", ",", " ", "NonGen", ",", " ", "vec", ",", " ", 
        "vec1", ",", " ", "dimMat", ",", " ", "NPA", ",", "symmetrise", ",", 
        " ", "choice", ",", " ", "uniqueElements", ",", "numElements", ",", 
        " ", "mymap", ",", " ", "constraints"}], "}"}], ",", 
      "\[IndentingNewLine]", "\n", 
      RowBox[{
       RowBox[{
        RowBox[{"ClearAll", "[", "CircleTimes", "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"SetAttributes", "[", 
         RowBox[{"CircleTimes", ",", 
          RowBox[{"{", 
           RowBox[{"Flat", ",", "OneIdentity"}], "}"}]}], "]"}], ";", "\n", 
        RowBox[{"choice", " ", "=", " ", 
         RowBox[{"ChoiceDialog", "[", 
          RowBox[{
          "\"\<Compute constraints for real or complex-valued variables?\>\"",
            ",", " ", "\n", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"\"\<Real\>\"", " ", "\[Rule]", 
              StyleBox[
               RowBox[{" ", 
                StyleBox[" ",
                 FontFamily->"Courier",
                 FontWeight->"Bold"]}]], "0"}], ",", " ", 
             RowBox[{"\"\<Complex\>\"", " ", "\[Rule]", 
              StyleBox[
               RowBox[{" ", 
                StyleBox[" ",
                 FontFamily->"Courier",
                 FontWeight->"Bold"]}]], "1"}]}], "}"}]}], "]"}]}], ";", " ", 
        "\[IndentingNewLine]", 
        RowBox[{"allrules", " ", "=", " ", 
         RowBox[{"{", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{"0", "\[CircleTimes]", " ", "a1_"}], "\[RuleDelayed]", 
            "0"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"a1_", "\[CircleTimes]", "0"}], "\[RuleDelayed]", "0"}], 
           ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{
             TemplateBox[{SubscriptBox["e", "q1_"],RowBox[{"{", 
                 RowBox[{"x_", ",", "a_"}], "}"}]},
              "Superscript"], "\[CircleTimes]", 
             TemplateBox[{SubscriptBox["e", "q1_"],RowBox[{"{", 
                 RowBox[{"x_", ",", "a1_"}], "}"}]},
              "Superscript"]}], "\[RuleDelayed]", 
            RowBox[{"0", "/;", " ", 
             RowBox[{"a", " ", "\[NotEqual]", "a1"}]}]}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"a1_", "\[CircleTimes]", "a2_"}], "\[RuleDelayed]", 
            RowBox[{"a2", "/;", 
             RowBox[{"a1", "\[Equal]", "\[CapitalIota]"}]}]}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"a1_", "\[CircleTimes]", "a2_"}], "\[RuleDelayed]", 
            RowBox[{"a1", "/;", 
             RowBox[{"a2", "\[Equal]", "\[CapitalIota]"}]}]}], ",", " ", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"\[CapitalIota]", "\[CircleTimes]", "\[CapitalIota]"}], 
            "\[RuleDelayed]", " ", "\[CapitalIota]"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{
             TemplateBox[{SubscriptBox["e", "q1_"],RowBox[{"{", 
                 RowBox[{"x_", ",", "a_"}], "}"}]},
              "Superscript"], "\[CircleTimes]", 
             TemplateBox[{SubscriptBox["e", "p1_"],RowBox[{"{", 
                 RowBox[{"y_", ",", "b_"}], "}"}]},
              "Superscript"]}], "\[RuleDelayed]", 
            RowBox[{
             RowBox[{"Sort", "[", 
              RowBox[{
               TemplateBox[{SubscriptBox["e", "p1"],RowBox[{"{", 
                   RowBox[{"y", ",", "b"}], "}"}]},
                "Superscript"], "\[CircleTimes]", 
               TemplateBox[{SubscriptBox["e", "q1"],RowBox[{"{", 
                   RowBox[{"x", ",", "a"}], "}"}]},
                "Superscript"]}], "]"}], "/;", " ", 
             RowBox[{"q1", ">", "p1"}]}]}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{
             TemplateBox[{SubscriptBox["e", "q1_"],RowBox[{"{", 
                 RowBox[{"x_", ",", "a_"}], "}"}]},
              "Superscript"], "\[CircleTimes]", 
             TemplateBox[{SubscriptBox["e", "q1_"],RowBox[{"{", 
                 RowBox[{"x_", ",", "a_"}], "}"}]},
              "Superscript"]}], "\[RuleDelayed]", 
            TemplateBox[{SubscriptBox["e", "q1"],RowBox[{"{", 
                RowBox[{"x", ",", "a"}], "}"}]},
             "Superscript"]}]}], "\[IndentingNewLine]", "}"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"Generators", "=", 
         RowBox[{"Sort", "@", 
          RowBox[{"Join", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
             "\[CapitalIota]", "\[CircleTimes]", " ", "\[CapitalIota]"}], 
             "}"}], ",", 
            RowBox[{"Flatten", "@", 
             RowBox[{"Table", "[", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"Table", "[", 
                RowBox[{
                 RowBox[{"Table", "[", 
                  RowBox[{
                   RowBox[{"{", 
                    RowBox[{"\[CapitalIota]", "\[CircleTimes]", 
                    TemplateBox[{SubscriptBox["e", "i"],RowBox[{"{", 
                    RowBox[{"j", ",", "k"}], "}"}]},
                    "Superscript"]}], "}"}], ",", 
                   RowBox[{"{", 
                    RowBox[{"j", ",", " ", "1", ",", " ", 
                    RowBox[{"inputs", "[", 
                    RowBox[{"[", "i", "]"}], "]"}]}], "}"}]}], "]"}], ",", 
                 RowBox[{"{", 
                  RowBox[{"k", ",", " ", "1", ",", "  ", 
                   RowBox[{
                    RowBox[{"outputs", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], "-", "1"}]}], "}"}]}], 
                "]"}], ",", 
               RowBox[{"{", 
                RowBox[{"i", ",", " ", "1", ",", " ", "parties"}], "}"}]}], 
              "]"}]}]}], "]"}]}]}], " ", ";", "\[IndentingNewLine]", "\n", 
        RowBox[{"LTuples", "=", 
         RowBox[{"Tuples", "[", 
          RowBox[{"Generators", ",", 
           RowBox[{"{", "level", "}"}]}], "]"}]}], ";", 
        RowBox[{"NumberLTuples", "=", 
         RowBox[{"Length", "[", "LTuples", "]"}]}], ";", 
        "\[IndentingNewLine]", "\n", 
        RowBox[{"joinTuples", "=", 
         RowBox[{
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"Apply", "[", 
             RowBox[{"CircleTimes", ",", " ", 
              RowBox[{"LTuples", "[", 
               RowBox[{"[", 
                RowBox[{"i", ",", " ", "All"}], "]"}], "]"}]}], "]"}], ",", 
            " ", 
            RowBox[{"{", 
             RowBox[{"i", ",", " ", "1", ",", " ", "NumberLTuples"}], "}"}]}],
            "]"}], "//.", "allrules"}]}], ";", " ", "\[IndentingNewLine]", 
        "\n", 
        RowBox[{"NonGen", "=", 
         RowBox[{
          RowBox[{
           RowBox[{"Sort", "[", 
            RowBox[{"#1", "[", 
             RowBox[{"[", 
              RowBox[{
               RowBox[{
                RowBox[{"Length", "[", "Generators", "]"}], "+", "1"}], ";;", 
               
               RowBox[{"Length", "[", "#1", "]"}]}], "]"}], "]"}], "]"}], 
           "&"}], "@", 
          RowBox[{"DeleteDuplicates", "@", 
           RowBox[{"Flatten", "@", 
            RowBox[{"Gather", "[", "joinTuples", "]"}]}]}]}]}], ";", "\n", 
        "\n", 
        RowBox[{"vec", "=", " ", 
         RowBox[{"Join", "[", 
          RowBox[{"Generators", ",", 
           RowBox[{"DeleteCases", "[", 
            RowBox[{"NonGen", ",", "0"}], "]"}]}], "]"}]}], ";", "\n", 
        RowBox[{"vec1", " ", "=", " ", 
         RowBox[{"Reverse", "/@", " ", "vec"}]}], ";", "\n", 
        RowBox[{"dimMat", " ", "=", " ", 
         RowBox[{"Length", "[", "vec", "]"}]}], ";", "\[IndentingNewLine]", 
        "\n", 
        RowBox[{"NPA", "=", 
         RowBox[{
          RowBox[{"Outer", "[", 
           RowBox[{"CircleTimes", ",", "vec1", ",", "vec"}], "]"}], "//.", 
          "allrules"}]}], " ", ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"symmetrise", "[", "UpperTriMat_", "]"}], ":=", 
         RowBox[{"UpperTriMat", "+", 
          RowBox[{"Transpose", "[", "UpperTriMat", "]"}], "-", 
          RowBox[{"DiagonalMatrix", "[", 
           RowBox[{"Flatten", "[", 
            RowBox[{"MapIndexed", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"#1", "[", 
                RowBox[{"[", "#2", "]"}], "]"}], "&"}], ",", "UpperTriMat"}], 
             "]"}], "]"}], "]"}]}]}], ";", "\n", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"SameQ", "[", 
           RowBox[{"choice", ",", " ", "0"}], "]"}], ",", " ", 
          RowBox[{"NPA", " ", "=", 
           RowBox[{"symmetrise", "[", 
            RowBox[{"UpperTriangularize", "[", "NPA", "]"}], "]"}]}], ",", 
          " ", "NPA"}], "]"}], ";", " ", "\n", "\n", 
        RowBox[{"uniqueElements", "=", 
         RowBox[{"DeleteDuplicates", "@", 
          RowBox[{"Flatten", "@", 
           RowBox[{"Gather", "[", "NPA", "]"}]}]}]}], ";", "\n", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"MemberQ", "[", 
           RowBox[{"uniqueElements", ",", "0"}], "]"}], ",", 
          RowBox[{"uniqueElements", " ", "=", " ", 
           RowBox[{"Prepend", "[", 
            RowBox[{
             RowBox[{"DeleteCases", "[", 
              RowBox[{"uniqueElements", ",", " ", "0"}], "]"}], ",", "0"}], 
            "]"}]}], ",", " ", "uniqueElements"}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"numElements", " ", "=", " ", 
         RowBox[{"Length", "[", "uniqueElements", "]"}]}], ";", "\n", 
        RowBox[{
         RowBox[{"mymap", "[", "x_", "]"}], ":=", 
